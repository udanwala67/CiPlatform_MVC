@{
    var ratingScore = 0;

    if (Model != null)
    {
        ratingScore = Model;
    }
}

@for (var i = 0; i < ratingScore; i++)
{
    <h4 class="bi bi-star-fill text-warning mx-1" role="button" onclick="UserRating(@i, @ViewBag.missionId, @ViewBag.uid)"></h4>
}
@for (var i = ratingScore + 1; i <= 5; i++)
{
    <h4 class="bi bi-star mx-1" role="button" onclick="UserRating(@i, @ViewBag.missionId, @ViewBag.uid)"></h4>
}